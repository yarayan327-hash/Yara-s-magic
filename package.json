{
  "name": "yara-magic-ai",
  "version": "1.0.0",
  "description": "AI-powered magical application with text analysis, generation, and educational features",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "natural": "^6.5.0",
    "compromise": "^14.10.0",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "keywords": ["ai", "education", "nlp", "text-analysis", "magic"],
  "author": "Yara",
  "license": "MIT"
}